{
  "name": "muon-node-js",
  "version": "1.0.0",
  "description": "Muon protocol javascript implementation",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd babel-node ./index.js",
    "dev": "env-cmd nodemon --exec babel-node ./index.js",
    "dev-node-1": "env-cmd -f ./dev-chain/dev-node-1.env babel-node index.js",
    "dev-node-2": "env-cmd -f ./dev-chain/dev-node-2.env babel-node index.js",
    "dev-chain": "concurrently --kill-others \"npm run dev-node-1\" \"npm run dev-node-2\"",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nodeutils/defaults-deep": "^1.1.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chokidar": "^3.5.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ethers": "^5.1.4",
    "events": "^3.3.0",
    "events-async": "^1.2.1",
    "express": "^4.17.1",
    "it-length-prefixed": "^3.1.0",
    "it-pipe": "^1.1.0",
    "libp2p": "^0.30.11",
    "libp2p-bootstrap": "^0.12.3",
    "libp2p-gossipsub": "^0.8.0",
    "libp2p-kad-dht": "^0.22.0",
    "libp2p-mplex": "^0.10.2",
    "libp2p-noise": "^2.0.5",
